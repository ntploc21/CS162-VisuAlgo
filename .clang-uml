compilation_database_dir: build
output_directory: docs/diagrams
generate_method_arguments: none
diagrams:
  GUI:
    type: include
    relative_to: src
    glob:
      - src/*.cpp
    include_relations_also_as_members: false
    generate_system_headers: false
    include:
      paths:
        - src
    exclude:
      paths:
        - build
  Algorithm:
    type: class
    include_relations_also_as_members: false
    include:
      namespaces:
        - Algorithm
  Animation:
    type: class
    include_relations_also_as_members: false
    include:
      namespaces:
        - Animation
  Core:
    type: class
    include_relations_also_as_members: false
    include:
      namespaces:
        - Core